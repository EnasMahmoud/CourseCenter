<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="pro.phoenix.coursecenter">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MenuActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RoomsListActivity" />
        <activity android:name=".CustomersListActivity" />
        <activity android:name=".BookActivity" />
        <activity android:name=".AddRoomActivity" />
        <activity android:name=".EditRoomActivity" />
        <activity android:name=".AddCustomerActivity" />
        <activity android:name=".EditCustomerActivity" />
        <activity
            android:name=".RoomsTabActivity"
            android:label="@string/title_activity_rooms_tab"
            android:theme="@style/AppTheme"></activity>
    </application>

</manifest>